{
  "ignition": { "version": "2.0.0" },
  "systemd": {
    "units": [
      {
        "name": "locksmithd.service",
        "mask": true
      },
      {
        "name": "update-engine.service",
        "enabled": true
      }
    ]
  },
  "networkd": {
    "units": [
      {
        "name": "00-eth.network",
        "contents": "[Match]\nName=eno*\n\n[Network]\nBond=bond0"
      },
      {
        "name": "10-bond0.netdev",
        "contents": "[NetDev]\nName=bond0\nKind=bond\nMode=balance-rr"
      },
      {
        "name": "20-bond0.network",
        "contents": "[Match]\nName=bond0\n\n[Network]\nDHCP=true"
      }
    ]
  },
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa  blablu@blablus-MacBook-Pro.local",
          "ssh-rsa  rsa-key-olb-desktop"
        ]
      }
    ]
  }
}
